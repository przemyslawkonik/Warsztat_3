<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<table border="1">
 <tr>
  <th scope="col">Title</th>
  <th scope="col">Updated</th>
  <th scope="col"></th>
 </tr>
 <c:forEach var="s" items="${solutions}">
  <tr>
   <td>${s.title}</td>
   <td>${s.updated}</td>
   <td><a href="/Warsztat_3/solutionDetails?excercise_id=${s.excerciseId}&solution_id=${s.solutionId}">details</a></td>
  </tr>
 </c:forEach>
</table>